{
    "name": "website",
    "type": "module",
    "version": "0.0.1",
    "scripts": {
        "dev": "astro dev",
        "build": "astro build",
        "preview": "astro preview",
        "check::fmt": "prettier --check .",
        "check::lint": "eslint .",
        "check": "s=0; bun run check::lint || s=1; bun run check::fmt || s=1; exit $s",
        "fix::fmt": "prettier --write .",
        "fix::lint": "eslint --fix .",
        "fix": "bun run fix::lint && bun run fix::fmt"
    },
    "dependencies": {
        "@eslint/js": "^9.39.1",
        "@tailwindcss/vite": "^4.1.17",
        "astro": "^5.16.3",
        "tailwindcss": "^4.1.17"
    },
    "devDependencies": {
        "@typescript-eslint/parser": "^8.48.0",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-astro": "^1.5.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "prettier": "^3.7.3",
        "prettier-plugin-astro": "^0.14.1",
        "prettier-plugin-tailwindcss": "^0.7.1",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.48.0"
    }
}
