---
interface Props {
    title: string;
}

const { title } = Astro.props;
---

{
    title && (
        <>
            <title>{title}</title>
            <meta property="og:title" content={title} />
        </>
    )
}
<meta property="og:url" content={Astro.url.href} />

{/* Placeholder until profile picture is ready */}
<meta property="og:image" content="https://rustacean.net/assets/corro.svg" />
